{
  "name": "linter-textlint",
  "version": "1.13.0",
  "private": true,
  "description": "A plugin for Atom Linter providing an interface to textlint.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1000ch/linter-textlint.git"
  },
  "engines": {
    "atom": ">=1.0.0"
  },
  "scripts": {
    "test": "apm test",
    "lint": "eslint ."
  },
  "keywords": [
    "text",
    "lint",
    "textlint"
  ],
  "author": {
    "name": "1000ch",
    "email": "shogo.sensui@gmail.com",
    "url": "http://github.com/1000ch"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1000ch/linter-textlint/issues"
  },
  "homepage": "https://github.com/1000ch/linter-textlint#readme",
  "dependencies": {
    "atom-linter": "^6.0.0",
    "atom-package-deps": "^4.0.1",
    "escape-html": "^1.0.3",
    "textlint": "^7.0.1"
  },
  "devDependencies": {
    "eslint": "^3.0.1",
    "eslint-config-airbnb-base": "^4.0.0",
    "eslint-plugin-import": "^1.10.2",
    "textlint-rule-spellcheck-tech-word": "^5.0.0"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "global-require": 0,
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "atom"
          ]
        }
      ]
    },
    "extends": "airbnb-base",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
